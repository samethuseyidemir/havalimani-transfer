@{
    ViewData["Title"] = "Google Giriş Sonucu";
}

<h2>Google ile Giriş Başarılı!</h2>

<table class="table">
    <thead>
        <tr>
            <th>Bilgi Türü</th>
            <th>Değer</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var claim in ViewBag.User)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </tbody>
</table>

<a href="/logout" class="btn btn-danger">Çıkış Yap</a>
